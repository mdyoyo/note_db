<!DOCTYPE html>
<html>
<head>
    <title></title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <script src="http://code.jquery.com/jquery-latest.js"></script>
</head>
<body>
<script type="text/javascript">
    function get_jsonp() {
        $.getJSON("http://127.0.0.1:3000/jsonp?callback=?",
        function(data) {
            $('#rjsonp').val('Jsonp info : ' + data.status);
        });
    }

    function get_json(){
        $.getJSON("json",
                function(data) {
                    $('#rjson').val('Json info : ' + data.status);
                });
    }
    function get_json2(){
        $.getJSON("checkUsername_register",{username_r:"wangchen1"},
                function(data) {
                    $('#rjson').val('Json info : ' + data.msg);
                });
    }
</script>

<a href="javascript:get_jsonp();">Click me for jsonp</a><br />
<textarea  id="rjsonp" cols="50" rows="3"></textarea>

<a href="javascript:get_json2();">Click me for json</a><br />
<textarea id="rjson" cols="50" rows="3"></textarea>

</body>  
</html>